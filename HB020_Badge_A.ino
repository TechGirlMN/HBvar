void beep(int tone, int duration)
{
  for (long i = 0; i < duration * 900L; i += tone * 1)
  {
    digitalWrite(buzzerPin, HIGH);
    delayMicroseconds(tone*(.50));
    digitalWrite(buzzerPin, LOW);
    delayMicroseconds(tone*(.50));
  }
  delay(30);
}
 
